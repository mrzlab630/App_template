{
  "name": "application_start_template",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=10.14.2"
  },
  "browserslist": "> 0.25%, not dead",
  "author": "mrZ <mrzlab630@gmail.com>",
  "license": "MIT",
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "typescript",
    "react",
    "sass",
    "less",
    "css"
  ],
  "main": "dist/index.js",
  "scripts": {
    "myIp": "node ip.js",
    "copy_static_assets": "node copyStaticAssets.js",
    "start": "nodemon dist/index.js",
    "startDev": "cross-env NODE_ENV=development concurrently \"webpack --mode development --config webpack_config/webpack.dev.config.js\" \"nodemon dist\"",
    "dev": "cross-env NODE_ENV=development concurrently \"webpack --mode development --config webpack_config/webpack.dev.config.js --watch\" \"nodemon dist\"",
    "devServer": "yarn startDev & webpack-dev-server --config webpack_config/webpack.dev.config.js",
    "prod": "webpack --mode production --config webpack_config/webpack.prod.config.js",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "openLinux": "xdg-open http://localhost:4000",
    "openMac": "open http://localhost:4000",
    "openWin": "start http://localhost:4000"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "rotating-file-stream": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^3.0.3",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^5.0.0"
  }
