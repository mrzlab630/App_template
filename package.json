{
  "name": "application_start_template_ssr",
  "version": "2.1.0",
  "description": "template app: server side rendering",
  "engines": {
    "node": ">=10.14.2"
  },
  "browserslist": "> 0.25%, not dead",
  "browser": {
    "child_process": false
  },
  "author": "mrZ <mrzlab630@gmail.com>",
  "license": "MIT",
  "keywords": [
    "server side rendering",
    "graphql",
    "webpack",
    "babel",
    "eslint",
    "typescript",
    "react",
    "sass",
    "less",
    "css",
    "enzyme",
    "mocha",
    "chai"
  ],
  "scripts": {
    "openInBrowser": "node openInBrowser.js",
    "myIp": "node ip.js",
    "copy_static_assets": "node copyStaticAssets.js",
    "create_index_file": "node createFile.js",
    "dev:start": "nodemon  dist/index.js",
    "dev:build": "cross-env NODE_ENV=development webpack --mode development --config webpack_config/webpack.dev.config.js",
    "dev": "concurrently \"yarn create_index_file\" \"yarn dev:build\" \"yarn dev:start\"",
    "prod:build": "cross-env NODE_ENV=production webpack --mode production --config webpack_config/webpack.prod.config.js",
    "prod:start": "NODE_ENV=production node dist/index.js",
    "test": "webpack --config webpack.test.js && concurrently \"webpack --config webpack.test.js\" \"mocha --watch\"",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
